{
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico",
      "status": 200
    },
    {
      "src": "/diagnostic.html",
      "dest": "/diagnostic.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}